import java.util.Scanner;

public class Sandbox {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        Integer input = scan.nextInt();
        scan.close();

        for (int i = 1; i <= input; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print(j + " ");
            }
            System.out.println();
        }
    }
}