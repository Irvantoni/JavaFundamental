import java.util.Scanner;

public class Sandbox {
    public static void main(String[] args) {
        Scanner baca = new Scanner(System.in);
        Integer angka = baca.nextInt();
        baca.close();

        while (angka >= 1) {
            System.out.println(angka + " - \"I will become a java developer\"");
            angka--;
        }
    }
}